{
  "name": "bbog-kata-users-mngr",
  "version": "1.0.0",
  "main": "build/bin/www.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && export DEBUG=bdb:* && ts-node -r tsconfig-paths/register src/bin/www.ts",
    "dev": "tsx watch src/bin/www.ts",
    "test": "vitest run",
    "test:debug": "DEBUG=* npm test",
    "test:watch": "vitest watch --coverage",
    "coverage": "vitest run --coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "node -r tsx/register src/db/seed.ts",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "lint:fix": "npx eslint '*/**/*.ts' --fix",
    "prettier:all": "prettier \"**/*.{ts,json,html,scss,md}\" --write",
    "prettier": "prettier \"**/*.{ts,json,html,scss,md}\" --check",
    "fix:all": "npm run prettier:all && npm run lint:fix",
    "sonar:local": "npm run coverage:report && npm run clean && sonar-scanner",
    "debug": "DEBUG=* node ./build/bin/www.js",
    "clean": "rimraf build .scannerwork",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\""
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "keywords": [
    "typescript",
    "express",
    "js",
    "node-js"
  ],
  "license": "ISC",
  "dependencies": {
    "aws-serverless-express": "^3.4.0",
    "drizzle-orm": "^0.44.1",
    "express": "^5.1.0",
    "express-intercept": "^1.1.1",
    "express-openapi-validate": "^0.6.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/debug": "^4.1.7",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.18",
    "@types/pg": "^8.15.2",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^3.1.3",
    "drizzle-kit": "^0.31.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-license-header": "^0.6.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "nock": "^13.4.0",
    "prettier": "^3.5.2",
    "rimraf": "^5.0.5",
    "sonarqube-scanner": "^4.3.0",
    "supertest": "^7.1.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.4",
    "typescript": "5.5.4",
    "vitest": "^3.1.3"
  },
  "overrides": {
    "glob": "^9.3.5",
    "rimraf": "^5.0.5",
    "inflight": "^2.0.1"
  }
}
